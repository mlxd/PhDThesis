@article{OL:Greiner_physb_2003,
	Abstract = {A quantum phase transition from a superfluid to a Mott insulating ground state was observed in a Bose-Einstein condensate stored in a three-dimensional optical lattice potential. With this experiment a new field of physics with ultracold atomic quantum gases is entered. Now interactions between atoms dominate the behavior of the many-body system, such that it cannot be described by the usual theories for weakly interacting Bose gases anymore. (C) 2003 Published by Elsevier Science B.V.},
