@article{VTX:Kwon_pra_2014,
	Abstract = {We investigate thermal relaxation of superfluid turbulence in a highly oblate Bose-Einstein condensate. We generate turbulent flow in the condensate by sweeping the center region of the condensate with a repulsive optical potential. The turbulent condensate shows a spatially disordered distribution of quantized vortices, and the vortex number of the condensate exhibits nonexponential decay behavior which we attribute to the vortex pair annihilation. The vortex-antivortex collisions in the condensate are identified with crescent-shaped, coalesced vortex cores. We observe that the nonexponential decay of the vortex number is quantitatively well described by a rate equation consisting of one-body and two-body decay terms. In our measurement, we find that the local two-body decay rate is closely proportional to T-2/mu, where T is the temperature and mu is the chemical potential.},
	Author = {Kwon, W. J. and Moon, G. and Choi, J. Y. and Seo, S. W. and Shin, Y. I.},
	Date-Modified = {2016-10-25 08:02:41 +0000},
	Journal = {Physical Review A},
