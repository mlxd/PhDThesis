	Bdsk-Url-1 = {http://link.aps.org/doi/10.1103/PhysRevA.86.013635%20files/170/e013635.html%20http://pra.aps.org/abstract/PRA/v86/i1/e013635}}

@article{SS:Yankowitz_natphys_2012,
	Abstract = {The Schrodinger equation dictates that the propagation of nearly free electrons through a weak periodic potential results in the opening of bandgaps near points of the reciprocal lattice known as Brillouin zone boundaries(1). However, in the case of massless Dirac fermions, it has been predicted that the chirality of the charge carriers prevents the opening of a bandgap and instead new Dirac points appear in the electronic structure of the material(2,3). Graphene on hexagonal boron nitride exhibits a rotation-dependent moire pattern(4,5). Here, we show experimentally and theoretically that this moire pattern acts as a weak periodic potential and thereby leads to the emergence of a new set of Dirac points at an energy determined by its wavelength. The new massless Dirac fermions generated at these superlattice Dirac points are characterized by a significantly reduced Fermi velocity. Furthermore, the local density of states near these Dirac cones exhibits hexagonal modulation due to the influence of the periodic potential.},
	Author = {Yankowitz, M. and Xue, J. M. and Cormode, D. and Sanchez-Yamagishi, J. D. and Watanabe, K. and Taniguchi, T. and Jarillo-Herrero, P. and Jacquod, P. and LeRoy, B. J.},
	Date-Modified = {2016-10-25 08:02:58 +0000},
	Journal = {Nature Physics},
	Keywords = {scanning-tunneling-microscopy electrons},
	Number = {5},
	Pages = {382-386},
	Title = {Emergence of superlattice Dirac points in graphene on hexagonal boron nitride},
	Volume = {8},
	Year = {2012},
	Bdsk-Url-1 = {%3CGo%20to%20ISI%3E://WOS:000303599200014}}

