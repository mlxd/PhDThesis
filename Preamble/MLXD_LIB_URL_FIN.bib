	Volume = {93},
	Year = {2004},
	Bdsk-Url-1 = {%3CGo%20to%20ISI%3E://WOS:000223138200001}}

@article{BEC:Rosenbusch_laserphys_2004,
	Abstract = {We study experimentally the line of a single quantized vortex in a rotating prolate Bose-Einstein condensate of Rb atoms confined by a harmonic potential. Exciting the two transverse quadrupole modes of the single vortex condensate, we show that the counterrotating mode is more strongly damped than in the absence of a vortex, whereas the corotating mode is not affected appreciably by the vortex. We interpret this result as a decay of the counterrotating quadrupole mode into two excitations of the vortex line, the so-called Kelvin modes. This is supported by direct observation of the vortex line.},
	Author = {Rosenbusch, P. and Bretin, V. and Chevy, F. and Shlyapnikov, G. V. and Dalibard, J.},
