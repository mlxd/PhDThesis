	Number = {2},
	Title = {Propagation of sound in a Bose-Einstein condensate in an optical lattice},
	Volume = {70},
	Year = {2004},
	Bdsk-Url-1 = {%3CGo%20to%20ISI%3E://WOS:000223717400109}}

@article{Vtx:Oktel_pra_2004,
	Abstract = {We study the vortex lattices in a Bose-Einstein condensate in a rotating anisotropic harmonic trap. We first investigate the single-particle wave functions obtained by the exact solution of the problem and give simple expressions for these wave functions in the small anisotropy limit. Depending on the strength of the interactions, a few or a large number of vortices can be formed. In the limit of many vortices, we calculate the density profile of the cloud and show that the vortex lattice stays triangular. We also find that the vortex lattice planes align themselves with the weak axis of the external potential. For a small number of vortices, we numerically solve the Gross-Pitaevskii equation and find vortex configurations that are very different from the vortex configurations in an axisymmetric rotating trap.},
	Author = {Oktel, M. O.},
	Date-Modified = {2016-10-25 08:02:48 +0000},
	Journal = {Physical Review A},
	Number = {2},
	Title = {Vortex lattice of a Bose-Einstein condensate in a rotating anisotropic trap},
	Volume = {69},
	Year = {2004},
	Bdsk-Url-1 = {%3CGo%20to%20ISI%3E://WOS:000189386300109%20http://pra.aps.org/abstract/PRA/v69/i2/e023618}}

