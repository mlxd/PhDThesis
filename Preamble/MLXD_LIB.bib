@techreport{
Vtx:Kuopanportti_arxiv_2012,
   Author = {Kuopanportti, Pekko and Huhtamäki, Jukka A. M. and Möttönen, Mikko},
   Title = {Exotic vortex lattices in two-species Bose-Einstein condensates},
   Note = {Comment: 7 pages, 4 color figures; the content of v2 is identical to the published article},
   Number = {1201.4974},
   Type = {arXiv e-print},
   Month= {2012/01/24/},
   Abstract = {We numerically investigate vortex lattices in rotating two-component Bose-Einstein condensates in which the two components have unequal atomic masses and interact attractively with each other. For sufficiently strong attraction, the system is found to exhibit exotic ground-state structures in a harmonic trap, such as lattices having a square geometry or consisting of two-quantum vortices. The obtained states satisfy the Feynman relation, and they can be realized with current experimental techniques.},
