@article{
AO:Lahaye_pra_2005,
   Author = {Lahaye, T. and Wang, Z. and Reinaudi, G. and Rath, S. P. and Dalibard, J. and Guery-Odelin, D.},
   Title = {Evaporative cooling of a guided rubidium atomic beam},
   Journal = {Physical Review A},
   Volume = {72},
