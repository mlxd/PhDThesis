@article{
BEC:Denschlag_sci_2000,
   Author = {Denschlag, J. and Simsarian, J. E. and Feder, D. L. and Clark, C. W. and Collins, L. A. and Cubizolles, J. and Deng, L. and Hagley, E. W. and Helmerson, K. and Reinhardt, W. P. and Rolston, S. L. and Schneider, B. I. and Phillips, W. D.},
   Title = {Generating solitons by phase engineering of a Bose-Einstein condensate},
