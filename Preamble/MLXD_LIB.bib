   Author = {Roberts, J. L. and Claussen, N. R. and Burke, J. P. and Greene, C. H. and Cornell, E. A. and Wieman, C. E.},
   Title = {Resonant magnetic field control of elastic scattering in cold Rb-85},
   Journal = {Physical Review Letters},
   Volume = {81},
   Number = {23},
   Pages = {5109-5112},
   Abstract = {A magnetic field-dependent Feshbach resonance has been observed in the elastic scattering collision rate between atoms in the F = 2, M = -2 state of Rb-85. Changing the magnetic field by several Gauss caused the collision rate to vary by a factor of 10(4), and the sign of the scattering length could be reversed. The resonance peak is at 155.2(4) G and its width is 11.6(5) G. From these results we extract much improved values for the three quantities that characterize the interaction potential: The van der Waals coefficient C-6, the singlet scattering length a(S), and the tripler scattering length a(T). [S0031-9007(98)07904-6].},
