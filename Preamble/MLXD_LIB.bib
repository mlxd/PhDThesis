@article{
AO:Jo_prl_2012,
   Author = {Jo, G. B. and Guzman, J. and Thomas, C. K. and Hosur, P. and Vishwanath, A. and Stamper-Kurn, D. M.},
   Title = {Ultracold Atoms in a Tunable Optical Kagome Lattice},
   Journal = {Physical Review Letters},
   Volume = {108},
   Number = {4},
   Abstract = {We realize a two-dimensional kagome lattice for ultracold atoms by overlaying two commensurate triangular optical lattices generated by light at the wavelengths of 532 and 1064 nm. Stabilizing and tuning the relative position of the two lattices, we explore different lattice geometries including a kagome, a one-dimensional stripe, and a decorated triangular lattice. We characterize these geometries using Kapitza-Dirac diffraction and by analyzing the Bloch-state composition of a superfluid released suddenly from the lattice. The Bloch-state analysis also allows us to determine the ground-state distribution within the superlattice unit cell. The lattices implemented in this work offer a near-ideal realization of a paradigmatic model of many-body quantum physics, which can serve as a platform for future studies of geometric frustration.},
